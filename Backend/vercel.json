{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/404.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 60
    },
    "api/*.js": {
      "maxDuration": 10
    }
  }
}